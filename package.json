{
  "name": "sanity-plugin-internationalized-array",
  "version": "0.0.7",
  "description": "Store localized fields in an array to save on attributes",
  "main": "lib/index.js",
  "scripts": {
    "build": "sanipack build",
    "verify": "sanipack verify",
    "watch": "sanipack build --watch",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable && sanipack build && sanipack verify",
    "postpublish": "pinst --enable",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SimeonGriggs/sanity-plugin-internationalized-array.git"
  },
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@sanity/icons": "^1.3.1",
    "@sanity/ui": "^0.37.9",
    "styled-components": "^5.3.5"
  },
  "peerDependencies": {
    "@sanity/base": "^2.30.1",
    "@sanity/desk-tool": "^2.30.1",
    "@sanity/form-builder": "^2.30.1",
    "@sanity/util": "^2.29.5",
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^2.0.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "6.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "sanipack": "^2.1.0",
    "typescript": "^4.7.4"
  },
  "bugs": {
    "url": "https://github.com/SimeonGriggs/sanity-plugin-internationalized-array/issues"
  },
  "homepage": "https://github.com/SimeonGriggs/sanity-plugin-internationalized-array#readme"
}
